<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="600.0"
            prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controllers.MainMenuLayoutController">
   <top>
      <HBox prefHeight="26.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#showDirectoryMenu" text="Справочиник" />
            <Button mnemonicParsing="false" onAction="#showInvoicesMenu" text="Докумены" />
            <Button mnemonicParsing="false" onAction="#showJournalsMenu" text="Журналы" />
            <Button mnemonicParsing="false" onAction="#showReportsMenu" text="Отчеты" />
         </children>
         <BorderPane.margin>
            <Insets bottom="25.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
