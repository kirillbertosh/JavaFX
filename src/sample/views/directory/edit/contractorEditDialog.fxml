<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.controllers.directory.edit.ContractorEditDialogController"
            prefHeight="200.0" prefWidth="400.0">
    <children>
        <GridPane layoutX="165.0" layoutY="66.0" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Номер" />
                <Label text="Название" GridPane.rowIndex="1" />
                <Label text="Телефон" GridPane.rowIndex="2" />
                <TextField fx:id="numberField" GridPane.columnIndex="1" />
                <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="phoneField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
        <HBox layoutX="174.0" layoutY="161.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="45.0">
            <children>
                <Button layoutX="174.0" layoutY="161.0" mnemonicParsing="false" onAction="#okClicked" text="Ок" />
                <Button layoutX="274.0" layoutY="161.0" mnemonicParsing="false" onAction="#handleCancel" text="Отмена" />
            </children>
        </HBox>
    </children>
</AnchorPane>
